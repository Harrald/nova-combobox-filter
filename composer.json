{
    "name": "harrald/nova-combobox-filter",
    "description": "A Laravel Nova combobox filter. Supports selecting multiple items",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Harrald\\NovaComboboxFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harrald\\NovaComboboxFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
